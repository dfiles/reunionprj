<h1>Listing of Missing Classmates</h1>

<table>
  <tr>
    <th>CHS Name</th>
    <th>Current Last Name</th>
    <th>Profile status</th>
  </tr>

<% @profiles.each do |profile| %>
  <tr>
    <td><%=h profile.first_name + " " + profile.last_name%></td>
    <td><%=h profile.current_l_name%></td>
    <td><%=h profile.profile_status %></td>
  </tr>
  <% end %>
</table>
<br />
