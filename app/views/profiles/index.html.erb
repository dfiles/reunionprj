<h1>Listing profiles</h1>

<table>
  <tr>
    <th>* </th>
    <th>CHS Name</th>
    <th>Current name</th>
    <th>Email</th>
    <th>Private ?</th>
    <th>Profile status</th>
  </tr>

<% @profiles.each do |profile| %>
  <tr>
    <td><%= link_to 'Edit', edit_profile_path(profile) %></td>
    <td><%=h profile.first_name + " " + profile.last_name%></td>
    <td><%=h profile.current_f_name + " " + profile.current_l_name%></td>
    <td><%=h profile.email %></td>
    <td><%=h profile.priv_flg %></td>
    <td><%=h profile.profile_status %></td>
    <td><%= link_to 'Show', profile %></td>
    <td><%= link_to 'Destroy', profile, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
  <% end %>
</table>
<br />

<%= link_to 'New profile', new_profile_path %>
<div id="tnt_pagination">
<%= will_paginate @profile %>
</div>